<%- include('../partials/header.ejs') %>
<div class="auth-page">
  <h2>Sign In</h2>
  <div>
    <form action="/auth/sign-in" method="POST">
      <img src="/images/Icons/seenItLogo.png" alt="SeenIt" />
      <input class="auth-input card-hover" type="email" name="email" placeholder="Email" required />
      <input class="auth-input card-hover" type="password" name="password" placeholder="Password" required />
      <button class="btn-style card-hover" type="submit">Sign In</button>
    </form>
  </div>
</div>
<%- include('../partials/footer.ejs') %>
